<p id="notice"><%= notice %></p>

<h1>Agenda Examen2</h1>

<table class="highlight" >
  <thead>
    <tr>
     
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Imagen</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactos.each do |contacto| %>
      <tr>
        
        <td><%= contacto.name %></td>
        <td><%= contacto.email %></td>
       <td><%= contacto.phone %></td>
       <td>
         <img src="<%= contacto.imagen %>" width="50"/>
       </td>
        <td><a href= "/contactos/<%= contacto.id%>"class="btn-floating btn-large waves-effect 	            waves-light red"><i class="material-icons">pageview
           </i>
           </a>
	</td>
        <td><a href= "/contactos/<%= contacto.id%>/edit" class="btn-floating btn-large waves-    	  effect waves-light red"><i class="material-icons">mode_edit
          </i>
         </a>
        </td>
        <td><a data-method="delete" href =/contactos/<%= contacto.id %> data-	   	  confirm="Seguro?"class="btn-floating btn-large waves-effect waves-light red">
         <i class="material-icons">delete
         </i>
         </a>
      </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a href="/contactos/new" class="btn-floating btn-large waves-effect waves-light red">
 <i class="material-icons">add</i>
</a>
